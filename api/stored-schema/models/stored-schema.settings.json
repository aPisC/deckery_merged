{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "stored_schemas",
  "info": {
    "name": "StoredSchema"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "key": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "gameSchema": {
      "type": "json"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "json"
    },
    "cacheImage": {
      "type": "json",
      "private": true
    }
  }
}
